<?php
// app/dto/ArchivoDTO.php

class ArchivoDTO {
    private $id;
    private $id_contrato;
    private $nombre;
    private $ruta;

    public function __construct($id, $id_contrato, $nombre, $ruta) {
        $this->id = $id;
        $this->id_contrato = $id_contrato;
        $this->nombre = $nombre;
        $this->ruta = $ruta;
    }

    public function getId() {
        return $this->id;
    }

    public function getIdContrato() {
        return $this->id_contrato;
    }

    public function getNombre() {
        return $this->nombre;
    }

    public function getRuta() {
        return $this->ruta;
    }

    public function setId($id) {
        $this->id = $id;
    }

    public function setIdContrato($id_contrato) {
        $this->id_contrato = $id_contrato;
    }

    public function setNombre($nombre) {
        $this->nombre = $nombre;
    }

    public function setRuta($ruta) {
        $this->ruta = $ruta;
    }
}
?>
